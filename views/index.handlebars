<div>
    <h3 class="card z-depth-3 center-align">Read the Latest from 'The New York Times Technology Section'</h3>
    
</div>
<div class="row" id="articleContainer">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        {{#unless saved}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <button class="waves-effect waves-light btn saveArticle" data-id={{this.id}}>Save Article</button>
            <p>{{this.summary}}</p>
            <a href={{this.link}} target="_blank">{{this.link}}</a>
        </div>
        {{/unless}}
        {{/each}}
    </div>
</div>

