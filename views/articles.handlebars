
{{#each article}}
{{#unless saved}}
<div class="col s12 m6">
    <div class="card blue-grey darken-1">
        <div class="card-content white-text">
            <span class="card-title">{{title}}</span>
            <p>{{summary}}</p>
        </div>
        <div class="card-action">
            <a href={{link}} target="blank">{{link}}</a>

        </div>
    </div>
    {{/unless}}
    {{/each}}
        
</div>