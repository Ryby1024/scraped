<div class="row" id="savedArticles">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}

        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <button class="waves-effect waves-light btn modal-trigger checkNotes" href="#noteModal" data-id={{this.id}}>View or Add a Note</button>
            <button class="waves-effect waves-light btn deleteArticle" data-id={{this.id}}>Delete from Saved</button>
            <p>{{this.summary}}</p>
            <a href={{this.link}} target="_blank">{{this.link}}</a>
            
        </div>

        {{/each}}
    </div>
</div>
{{#each article}}
{{> notemodal }}
{{/each}}