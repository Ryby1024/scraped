<div class="row" id="savedArticles">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        {{#if saved}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <button class="waves-effect waves-light btn" id="addNote">Add Note</button>
            <button class="waves-effect waves-light btn" id="commentBtn">Delete from Saved</button>
            <p>{{this.summary}}</p>
        </div>
        {{/if}}
        {{/each}}
    </div>
</div>